  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "get-restaurants",
      "program": "${workspaceFolder}\\node_modules\\serverless\\bin\\serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "get-restaurants",
        "-d",
        "{}"
      ],
      "env": {
        "restaurants_table": "restaurants"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "search-restaurants",
      "program": "${workspaceFolder}\\node_modules\\serverless\\bin\\serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "search-restaurants",
        "-p",
        "examples/search-restaurants.json"
      ],
      "env": {
        "restaurants_table": "restaurants"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "get-index",
      "program": "${workspaceFolder}\\node_modules\\serverless\\bin\\serverless",
      "args": [
        "invoke",
        "local",
        "-f",
        "get-index",
        "-d",
        "{}"
      ],
      "env": {
        "restaurants_api": "https://y94uwslqk5.execute-api.us-east-1.amazonaws.com/dev/restaurants",
        "cognito_user_pool_id": "us-east-1_w0oiBcQQm",
        "cognito_client_id": "gkci83lekc1o8s5mvitmf2l6b"
      }
    }
  ]
}